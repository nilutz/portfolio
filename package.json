    "gatsby-plugin-sass": "^1.0.12",
    "gatsby-plugin-sharp": "^1.6.9",
    "gatsby-plugin-styled-components": "^1.0.5",
    "gatsby-remark-autolink-headers": "^1.4.7",
    "gatsby-remark-copy-linked-files": "^1.5.7",
    "gatsby-remark-images": "^1.5.47",
    "gatsby-remark-prismjs": "^1.2.8",
    "gatsby-remark-responsive-iframe": "^1.4.8",
    "gatsby-source-filesystem": "^1.5.3",
    "gatsby-transformer-remark": "^1.7.15",
    "html-loader": "^0.5.5",
    "lodash": "^4.17.4",
