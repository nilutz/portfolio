  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx": "0.0.2",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "gh-pages": "^1.0.0",
    "prettier": "^1.7.4"
  },
  "keywords": [
    "gatsby",
    "react",
    "portfolio"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "build:pp": "rm -rf .cache/* && gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "deploy:subtree": "git push origin `git subtree split --prefix public master`:gh-pages --force",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "lint": "eslint src/**/*/*.{js,jsx}",
    "lint:fix": "eslint  src/**/*/*.{js,jsx} --fix",
    "lint:p": "eslint *.{js,jsx}",
    "lint:p:fix": "eslint  *.{js,jsx} --fix",
    "test": "echo \"Error: no test for this specified\" && exit 1"
  }
